<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Smoke Test Suite" time-out="120000">

    <test name="Smoke Tests">

        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>



         <packages>
             <package name="tests"/>
         </packages>

    </test>
    

</suite>